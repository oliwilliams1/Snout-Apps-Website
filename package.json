{
  "name": "snout-apps-website",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"vite\" \"npm run build-css\" \"npm run build-ts\"",
    "build-css": "tailwindcss -i ./src/style.css -o ./dist/output.css --watch",
    "build-ts": "tsc --watch",
    "preview": "vite preview",
    "vite": "vite",
    "build": "tailwindcss -i ./src/todo/styles.css -o ./dist/todo/output.css && tsc && vite build",
    "git subtree push --prefix dist origin gh-pages": ""
  },
  "devDependencies": {
    "autoprefixer": "^10.4.20",
    "concurrently": "^7.3.0",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.14",
    "typescript": "~5.6.2",
    "vite": "^5.4.9"
  },
  "dependencies": {
    "@octokit/core": "^6.1.2",
    "color-calendar": "^1.0.7"
  }
}